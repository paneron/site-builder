{
  "name": "template-spa",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "compile": "tsc --outdir compiled; tsc --noEmit --module nodenext --skipLibCheck build.mts",
    "clean": "rm -rf dist compiled build.mjs dist.mjs build-site-builder.mjs build-site.mjs",
    "build-self-builder": "yarn esbuild build.mts --packages=external --platform=node --target=node18 --minify=false --bundle --format=esm --outfile=build.mjs",
    "build-self": "yarn node build.mjs",
    "build-site-builder-builder": "yarn esbuild build-site-builder.mts --packages=external --platform=node --target=node18 --minify=false --bundle --format=esm --outfile=build-site-builder.mjs",
    "build-site-builder": "yarn node build-site-builder.mjs",
    "clean-build": "yarn clean && yarn compile; yarn build",
    "build": "yarn build-self-builder && yarn build-self && yarn build-site-builder-builder && yarn build-site-builder"
  },
  "dependencies": {
    "@blueprintjs/core": "4.17.8",
    "@blueprintjs/datetime": "4.4.27",
    "@blueprintjs/icons": "4.14.5",
    "@blueprintjs/popover2": "1.13.12",
    "@blueprintjs/select": "4.9.12",
    "@blueprintjs/table": "4.10.12",
    "@effect/platform-browser": "^0.30.0",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@jsonurl/jsonurl": "^1.1.7",
    "@riboseinc/paneron-extension-kit": "2.2.29",
    "@riboseinc/paneron-registry-kit": "2.2.55",
    "@types/react": "18",
    "@types/react-dom": "18",
    "date-fns": "2.30.0",
    "date-fns-tz": "^2.0.0",
    "immer": "9.0.2",
    "immutability-helper": "3.1.1",
    "import-mapper": "0.0.4",
    "jspdf": "^2.5.1",
    "liquidjs": "9.25.0",
    "lz-string": "^1.5.0",
    "react": "18",
    "react-dom": "18",
    "react-helmet": "6.1.0",
    "react-mathjax2": "0.0.2",
    "react-resizable": "3.0.5",
    "react-virtualized-auto-sizer": "1.0.7",
    "react-visual-diff": "0.4.3",
    "react-window": "1.8.5",
    "rough-notation": "0.5.1",
    "semver": "7.3.8",
    "throttle-debounce": "3.0.1",
    "use-debounce": "9.0.4"
  },
  "devDependencies": {
    "@effect/cli": "~0.35.0",
    "@effect/platform": "~0.48.0",
    "@effect/platform-node": "~0.45.0",
    "@effect/printer": "~0.31.0",
    "@effect/printer-ansi": "~0.32.0",
    "@effect/schema": "~0.65.0",
    "@effect/typeclass": "~0.22.0",
    "@types/node": "20",
    "@types/react": "18",
    "@types/react-dom": "18",
    "effect": "~2.4.0",
    "esbuild": "0.19.9",
    "fast-check": "~3.15.0",
    "typescript": "~5.1",
    "yaml": "^2.3.4"
  }
}
