{
  "name": "@paneron/site-builder",
  "version": "0.0.1",
  "packageManager": "yarn@3.6.1",
  "type": "module",
  "bin": {
    "build-site": "./build-site.mjs"
  },
  "scripts": {
    "compile": "tsc --outdir compiled",
    "build": "sh -c 'yarn esbuild package.mts --external:esbuild --platform=node --target=node18 --minify=false --bundle --format=esm --outfile=package.mjs | yarn node package.mjs' ''"
  },
  "devDependencies": {
    "@types/node": "18",
    "esbuild": "^0.19.9",
    "typescript": "~5.0",
    "typescript-language-server": "~3.3.2"
  },
  "dependencies": {
    "@effect/cli": "^0.27.0",
    "@effect/platform-node": "^0.39.0",
    "@effect/schema": "^0.56.0",
    "effect": "^2.0.0-next.62",
    "fast-check": "^3.15.0"
  }
}
